<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes, minimum-scale=1.0, maximum-scale=5.0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>{{ title }}</title>
  <base href="{{ base }}" />
  {% if description %}<meta name="description" content="{{ description }}" />{% endif %}
  {% if robots %}  <meta name="robots" content="{{ robots }}" /><meta name="googlebot" content="{{ robots }}">{% endif %}
  {% if keywords %}<meta name="keywords" content="{{ keywords }}" />{% endif %}
  <link rel="stylesheet preload" as="style" href="catalog/view/theme/helium/css/stylesheet.css" media="screen" />
  <link rel="stylesheet preload" as="style" href="catalog/view/theme/helium/css/common.css" media="screen" />
  <link rel="stylesheet preload" as="style" href="catalog/view/theme/helium/css/fontello.css" media="screen" />
  {# <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin> #}
  {# <link rel="preconnect" href="https://fonts.googleapis.com"crossorigin> #}
  {# <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin>
  <link rel='preload' as='style' href='https://fonts.googleapis.com/css2?family=Roboto|Roboto+Condensed:wght@500&display=swap'>
  <link rel='stylesheet' href='https://fonts.googleapis.com/css2?family=Roboto|Roboto+Condensed:wght@500&display=swap'> #}
  {% if logo %}
  <link rel="preload" as="image" href="{{ logo }}">
  {% endif %}
  {# <script type="text/javascript" src="catalog/view/javascript/jquery/jquery-2.1.1.min.js" type="text/javascript"></script> #}
  {# <link rel="preload" href="catalog/view/theme/helium/css/fonts/fontello.woff2" as="font" type="font/woff2" crossorigin>  #}
  {% for style in styles %}
    <link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
  {% endfor %}
  {% for script in scripts %}
    <script defer src="{{ script }}" type="text/javascript"></script>
  {% endfor %}
  {% for link in links %}
    <link href="{{ link.href }}" rel="{{ link.rel }}" />
  {% endfor %}
  <meta property="og:title" content="{{ title }}" />
  <meta property="og:type" content="website" />
  <meta property="og:url" content="{{ og_url }}" />
  {% if og_image %}
  <meta property="og:image" content="{{ og_image }}" />
  {% else %}
  <meta property="og:image" content="{{ logo }}" />
  {% endif %}
  <meta property="og:site_name" content="{{ name }}" />
  {% for analytic in analytics %}
    {{ analytic }}
  {% endfor %}
  <script type="text/javascript">
    var js_lang = {
        load_more :"{{ text_load_more }}",
        text_discount_ends_in :"{{ text_discount_ends_in }}",
        days:  "{{ text_days }}",
        hours: "{{ text_hours }}",
        mins: "{{ text_mins }}",
        secs: "{{ text_secs }}",
        expand: "{{ text_expand }}",
        collapse: "{{ text_collapse }}",
        
        close: "{{ text_close }}",
        openlist: "{{ text_openlist }}",
        back_to: "{{ text_back_to }}",
    };
  </script>
</head>

<body>
<header>
  <nav id="top">
    <div role="presentation" class="logo">
      {% if logo %}
        <a href="{{ home }}">
          <img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" width="{{ logo_width }}" height="{{ logo_height }}" class="img-responsive" />
        </a>
      {% else %}
        <span><a href="{{ home }}">{{ name }}</a></span>
      {% endif %}
    </div>
    <div class="pull-right mobile_menu" data-block-name="{{ text_menu_settings }}" data-icon="icon-cart" data-order="0">
      {{ currency }}
      {{ language }}
      <a class="phone button" href="tel:{{ telephone }}" rel="phone" title="{{ text_phones }}" aria-label="{{ text_phones }}"><i class="icon-phone"></i><span class="hidden-md">{{ telephone }}</span></a>
      <div class="account-menu dropdown">
        <button data-accordion-target="account" type="button" title="{{ text_account }}" aria-label="{{ text_account }}">
          <i class="icon-user"></i>
          <span class="hidden-md">{{ text_account }}</span>
        </button>
        <div id="account_menu" data-accordion="account">
          {% if logged %}
            <a class="button" title="{{ text_account }}" href="{{ account }}"><span class=>{{ text_account }}</a>
            <a class="button" title="{{ text_order }}" href="{{ order }}">{{ text_order }}</a>
            <a class="button" title="{{ text_transaction }}" href="{{ transaction }}">{{ text_transaction }}</a>
            <a class="button" title="{{ text_download }}" href="{{ download }}">{{ text_download }}</a>
            <a class="button" title="{{ text_logout }}" href="{{ logout }}">{{ text_logout }}</a>
          {% else %}
            <a class="button" title="{{ text_register }}" href="{{ register }}">{{ text_register }}</a>
            <a class="button" title="{{ text_login }}" href="{{ login }}">{{ text_login }}</a>
          {% endif %}
        </div>
      </div>
      <button aria-haspopup="dialog" id="wishlist-total" title="{{ text_wishlist }}" aria-label="{{ text_wishlist }}"><i class="icon-heart"></i> <span class="hidden-md"> {{ text_wishlist_count }} </span></button>
      <button aria-haspopup="dialog" id="compare-total" title="{{ text_compare }}" aria-label="{{ text_compare }}"><i class="icon-compare"></i><span class="hidden-md">{{ text_compare_count }}</span></button>
    </div>
  </nav>
  <nav id="main">
    {{ menu }}
    <div class="search">
      {{ search }}
    </div>
    <div id="header-cart" class="cart">
      {{ cart }}
    </div>
  </nav>
</header>




