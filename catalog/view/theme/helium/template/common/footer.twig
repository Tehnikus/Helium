
<footer>
    
      {% if informations %}
      <div class="flex flex-wrap">
        <span class="h5 flex-1-0-100">{{ text_information }}</span>
        {% for row in informations|batch(informations|length / 4|round(1, 'ceil')) %}
        <div class="flex">
          <ul class="list-unstyled">
            {% for i in row %}
              <li><a href="{{ i.href }}">{{ i.title }}</a></li>
            {% endfor %}
          </ul>
        </div>
        {% endfor %}
        </div>
      {% endif %}
      <div class="flex">
      <div>
        <span class="h5">{{ text_service }}</span>
        <ul class="list-unstyled">
          <li><a href="{{ contact }}">{{ text_contact }}</a></li>
          <li><a href="{{ return }}">{{ text_return }}</a></li>
          <li><a href="{{ sitemap }}">{{ text_sitemap }}</a></li>
        </ul>
      </div>
      <div>
        <span class="h5">{{ text_extra }}</span>
        <ul class="list-unstyled">
          <li><a href="{{ manufacturer }}">{{ text_manufacturer }}</a></li>
          <li><a href="{{ voucher }}">{{ text_voucher }}</a></li>
          <li><a href="{{ affiliate }}">{{ text_affiliate }}</a></li>
          <li><a href="{{ special }}">{{ text_special }}</a></li>
        </ul>
      </div>
      <div>
        <span class="h5">{{ text_account }}</span>
        <ul class="list-unstyled">
          <li><a href="{{ account }}">{{ text_account }}</a></li>
          <li><a href="{{ order }}">{{ text_order }}</a></li>
          <li><a href="{{ wishlist }}">{{ text_wishlist }}</a></li>
          <li><a href="{{ newsletter }}">{{ text_newsletter }}</a></li>
        </ul>
      </div>
    </div>
    <hr>
    <p>{{ powered }}</p>
</footer>
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}
{% for script in scripts %}
<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}
  <script defer rel="preload" as="script" type="text/javascript" src="catalog/view/theme/helium/js/common.js" type="text/javascript"></script>

</body>
</html>